<?xml version='1.0'?>

<!DOCTYPE coherence SYSTEM "coherence.dtd">

<coherence>
  <security-config>
    <identity-transformer>
      <class-name>com.tangosol.examples.security.PasswordIdentityTransformer</class-name>
    </identity-transformer>
